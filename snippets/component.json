{
    "insertPage": {
      "scope": "json,jsonc",
      "prefix": ["!doPage"],
      "description": "Insert a page component",
      "body": [
        "{",
        "    \"Type\": \"page\",",
        "    \"Title\": \"${1|SPE_BO,SPE,BO|}_${2:$TM_FILENAME_BASE}\",",
        "    \"Events\": {",
        "        \"${3|AfterChildrenLoad,AfterLoad,AfterRender|}\": {",
        "            \"Script\": \"${2:$TM_FILENAME_BASE}.${4|id,new|}.AfterChildrenLoad\"",
        "        }",
        "    },",
        "    \"Children\": {",
        "        \"${2:$TM_FILENAME_BASE}New\": {",
        "            \"Type\": \"link\",",
        "            \"Label\": \"BO_New\",",
        "            \"Link\": {",
        "                \"Target\": \"${2:$TM_FILENAME_BASE}/new\"",
        "            }",
        "        }",
        "    }",
        "}"
      ]
    },
    "insertCrud": {
      "scope": "json,jsonc",
      "prefix": "!doCrud",
      "description": "Insert a crud component",
      "body": [
        "\"${1:$TM_FILENAME_BASE}Crud\": {",
        "   \"Type\": \"crud\",",
        "   ${2:\"CanInsert\": true,}",
        "   ${3:\"CanUpdate\": true,}",
        "   ${4:\"CanDelete\": true,}",
        "   \"Events\": {",
        "       \"BeforeInsert\": {",
        "           \"Script\": \"${1:$TM_FILENAME_BASE}.new.BeforeInsert\"",
        "       },",
        "       \"AfterInsert\": {",
        "           \"Script\": \"${1:$TM_FILENAME_BASE}.new.AfterInsert\"",
        "       },",
        "       \"BeforeUpdate\": {",
        "           \"Script\": \"${1:$TM_FILENAME_BASE}.id.BeforeUpdate\"",
        "       },",
        "       \"AfterUpdate\": {",
        "           \"Script\": \"${1:$TM_FILENAME_BASE}.id.AfterUpdate\"",
        "       },",
        "       \"BeforeDelete\": {",
        "           \"Script\": \"${1:$TM_FILENAME_BASE}.id.BeforeDelete\"",
        "       },",
        "       \"AfterDelete\": {",
        "           \"Script\": \"${1:$TM_FILENAME_BASE}.id.AfterDelete\"",
        "       }",
        "   },",
        "   \"Children\": {",
        "   }",
        "},"
      ]
    },
    "insertFilter": {
      "scope": "json,jsonc",
      "prefix": "!doFilter",
      "description": "Insert a filter component",
      "body": [
        "\"${1:$TM_FILENAME_BASE}Filter\": {",
        "   \"Type\": \"filters\",",
        "   \"Compact\": true,",
        "   \"Filters\": {",
        "       \"${1:$TM_FILENAME_BASE}.grid\": {",
        "           \"filterName\": {",
        "               \"DefaultFilter\": true,",
        "               \"Operator\": \"Between|Contains|DoesNotContain|EndsWith|Equal|Greater|GreaterOrEqual|In|Less|LessOrEqual|NotEqual|NotIn|NotNull|Null|StartsWith|...\",",
        "               \"FieldName\": \"datasourceFieldNameOrAlias\"",
        "           }",
        "       }",
        "   },",
        "   \"Children\": {",
        "       \"filterName\": {",
        "           \"Type\": \"checkbox|colorpicker|combobox|datepicker|emailbox|list|numberbox|password|radiobuttongroup|textbox|treeview|treeviewstandalone|...\",",
        "           \"Label\": \"sw_data_tablename_fieldname\"",
        "       }",
        "   }",
        "},"
      ]
    },
    "insertGrid": {
      "scope": "json,jsonc",
      "prefix": "!doGrid",
      "description": "Insert a grid component",
      "body": [
        "\"${1:$TM_FILENAME_BASE}Grid\": {",
        "   \"Type\": \"grid\",",
        "   \"DataSource\": \"${1:$TM_FILENAME_BASE}.grid\",",
        "   \"Columns\": {",
        "       \"datasourceFieldNameOrAlias\": {",
        "           \"title\": \"sw_data_tablename_fieldname|SPE_BO_\",",
        "           \"type\": \"color|string|number|date|boolean|...\",",
        "           \"Primary\": true,",
        "           \"Sortable\": false,",
        "           \"Exportable\": false,",
        "           \"Sum\": true,",
        "           \"count\": true,",
        "           \"FooterTemplate\": \"Gen_Total\",",
        "           \"GroupFooterTemplate\": \"BO_Subtotal\",",
        "           \"Link\": {",
        "               \"Target\": \"otherPage\"",
        "           },",
        "           \"Hidden\": true,",
        "           \"Width\": 100",
        "       },",
        "   }",
        "},"
      ]
    },
